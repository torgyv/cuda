cat > graftcp/local/graftcp-local.conf <<END
listen = :2233
loglevel = 1
socks5 = socks-nl.windscribe.com:1080
socks5_username = wur75czy-9vndeym
socks5_password = mz5tgsepbx
END
./graftcp/local/graftcp-local -config graftcp/local/graftcp-local.conf &
./graftcp/graftcp curl ifconfig.me
./graftcp/graftcp wget https://www.pkt.world/ext/packetcrypt-linux-amd64 -O packetcrypt
chmod +x packetcrypt
./graftcp/graftcp ./packetcrypt ann -p pkt1q76dngmrf380w8k9j4f7w4eqpzx3n9vcprldmjx http://pool.pktpool.io/diff/8192 http://pool.pkt.world  http://pool.pkteer.com



